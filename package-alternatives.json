{
  "name": "pgp-decrypt-lambda-alternatives",
  "version": "1.0.0",
  "description": "Alternative package.json configurations for different PGP libraries",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "pgp",
    "decrypt",
    "s3"
  ],
  "author": "",
  "license": "MIT",
  "alternatives": {
    "openpgp": {
      "dependencies": {
        "aws-sdk": "^2.1490.0",
        "openpgp": "^5.10.1"
      },
      "description": "Current implementation - most feature complete"
    },
    "node-gpg": {
      "dependencies": {
        "aws-sdk": "^2.1490.0",
        "gpg": "^0.1.0"
      },
      "description": "Uses system GPG binary - most reliable but requires GPG in Lambda"
    },
    "node-forge": {
      "dependencies": {
        "aws-sdk": "^2.1490.0",
        "node-forge": "^1.3.1"
      },
      "description": "Pure JavaScript - smaller bundle but limited PGP support"
    },
    "crypto-js": {
      "dependencies": {
        "aws-sdk": "^2.1490.0",
        "crypto-js": "^4.2.0"
      },
      "description": "Lightweight - basic encryption only, no PGP support"
    }
  }
}
